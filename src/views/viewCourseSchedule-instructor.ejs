<div class='container'>
   <div class='container-fluid mt-5'>
      <form id='addscheduleform' name='addscheduleform'>
         <div class='form-group'>
            <div class='row'>
               <div class='col'>
                  <a href="/instructor/dashboard/" class='btn btn-info btn-sm'>back</a>
               </div>
            </div>
         </div>
         <div class='form-group mt-3 mb-2' id="todayschedulediv">
            <div class="card border-primary mb-3">
               <div class="card-header"><i class="mr-2"><img id="extendtodayscheduletablebutton"
                        src="/static/images/extendBarUpBtn.svg" height="20" width="20" draggable="false"
                        alt="cardbodytodayschedulebutton" style="cursor: pointer;"></i>Today's course schedule</div>
               <div class="card-body" id="cardbodytodayschedulebutton">
                  <h4 id="todayschedulelabel" class="card-title"></h4>
                  <table id="todayscheduletable" class='table table-light tablefixed'>
                     <thead>
                        <tr>
                           <th scope="col">Time</th>
                           <th scope="col">Duration</th>
                           <th scope="col">Title</th>
                           <th scope="col">Cap</th>
                           <th scope='col'>Enrolled</th>
                        </tr>
                     </thead>
                     <tbody id="todayschedulebody">
                     </tbody>
                  </table>
                  <div id="todayscheduleemptymsgdiv" class="alert alert-warning alert-dismissible fade show hideRow"
                     role="alert">
                     <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                     </button>
                  </div>

               </div>
            </div>
         </div>
         <div class='form-group mt-2 mb-2' id="scheduletablelogdiv">
            <div class="card border-danger mb-3">
               <div class="card-header"><i class="mr-2"><img id="extendscheduletablelogbutton"
                        src="/static/images/extendBarDownBtn.svg" height="20" width="20" draggable="false"
                        alt="cardbodyscheduletablelogdiv" style="cursor: pointer;"></i>Schedule
                  logs</div>
               <div class="card-body hideRow" id="cardbodyscheduletablelogdiv">
                  <h4 id="scheduletablelogh4" class="card-title">
                     <div class="row">
                        <ul class="nav nav-pills" id="navfilter">
                           <li class="nav-item">
                              <a class="nav-link active" href="#">All</a>
                           </li>
                           <li class="nav-item">
                              <a class="nav-link" href="#">Sunday</a>
                           </li>
                           <li class="nav-item">
                              <a class="nav-link" href="#">Monday</a>
                           </li>
                           <li class="nav-item">
                              <a class="nav-link" href="#">Tuesday</a>
                           </li>
                           <li class="nav-item">
                              <a class="nav-link" href="#">Wednesday</a>
                           </li>
                           <li class="nav-item">
                              <a class="nav-link" href="#">Thursday</a>
                           </li>
                           <li class="nav-item">
                              <a class="nav-link" href="#">Friday</a>
                           </li>
                           <li class="nav-item">
                              <a class="nav-link" href="#">Saturday</a>
                           </li>
                        </ul>
                     </div>
                  </h4>
                  <div class="row">
                     <table id="scheduletablelogtable" class='table tablefixed'>
                        <thead>
                           <tr>
                              <th scope="col">Date</th>
                              <th scope="col">Day</th>
                              <th scope="col">Title</th>
                              <th scope="col">Time</th>
                              <th scope="col">Duration</th>
                              <th scope="col">Cap</th>
                              <th scope="col">Enrolled</th>
                           </tr>
                        </thead>
                        <tbody id="schedulelogbody">
                        </tbody>
                     </table>
                     <div id="scheduletablelogemptymsgdiv"
                        class="alert alert-warning alert-dismissible fade show hideRow" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                           <span aria-hidden="true">&times;</span>
                        </button>
                     </div>
                  </div>
               </div>
            </div>
         </div>
         <div class='form-group mt-2 mb-5' id="scheduletablediv">
            <div class="card border-dark mb-3">
               <div class="card-header"><i class="mr-2"><img id="extendscheduletablebutton"
                        src="/static/images/extendBarUpBtn.svg" height="20" width="20" draggable="false"
                        alt="scheduletablecardbodydiv" style="cursor: pointer;"></i>Calendar
               </div>
               <div class="card-body" id="scheduletablecardbodydiv">
                  <table class='table table-striped tablefixed'>
                     <thead>
                        <tr>
                           <th scope="col" class="thcss">week\day</th>
                           <th scope="col" class="tdcss">SUN</th>
                           <th scope="col" class="tdcss">MON</th>
                           <th scope="col" class="tdcss">TUE</th>
                           <th scope="col" class="tdcss">WED</th>
                           <th scope="col" class="tdcss">THU</th>
                           <th scope="col" class="tdcss">FRI</th>
                           <th scope="col" class="tdcss">SAT</th>
                        </tr>
                     </thead>
                     <tbody id='scheduletablebody'>
                     </tbody>
                  </table>
               </div>
            </div>
         </div>
      </form>
      <script src='/static/javascripts/viewCourseSchedule_instructor.js'></script>
   </div>
</div>